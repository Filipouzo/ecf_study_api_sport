{% extends 'base.html.twig' %}

{% block title %}GlobalOption{% endblock %}

{% block body %}
    <h1>GlobalOption</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ global_option.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ global_option.name }}</td>
            </tr>
            <tr>
                <th>Activated</th>
                <td>{{ global_option.activated ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_global_option_index') }}">back to list</a>

    <a href="{{ path('app_global_option_edit', {'id': global_option.id}) }}">edit</a>

    {{ include('global_option/_delete_form.html.twig') }}
{% endblock %}
